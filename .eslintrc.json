{
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
        "jsx": true,
        "modules": true,
        "experimentalObjectRestSpread": true
    }
  },
  "plugins": [
      "react"
  ],
  "extends": ["eslint:recommended", "plugin:react/recommended"],
  "rules": {
      "comma-dangle": 0,
      "no-unused-vars": 0,
      "react/prop-types": 0,
      "no-undef": 0,
      "no-console": 0
  },
  "settings": {
      "react": {
          "pragma": "React",
          "version": "15.4.2"
      }
  }
 }
 